{
  "name": "kwd",
  "version": "1.0.0",
  "scripts": {
    "test": "ts-node src/main.ts",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "allure open ./allure-report",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "test:worker1": "ts-node src/main.ts --worker=1/2",
    "test:worker2": "ts-node src/main.ts --worker=2/2",
    "test:parallel": "concurrently -n w1,w2 -c blue,magenta \"npm:test:worker1\" \"npm:test:worker2\"",
    "debug": "set PWDEBUG=1 && npm run test"
  },
  "keywords": [],
  "author": "",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "allure-commandline": "^2.34.1",
    "allure-js-commons": "^3.4.1",
    "concurrently": "^9.2.1",
    "csv-parse": "^6.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
